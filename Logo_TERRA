def seismik():
    terra.color("black", "#e5d4bb")
    terra.begin_fill()
    terra.seth(45)
    terra.circle(-270, 45)
    terra.right(90)
    terra.forward(350)
    terra.right(90)
    terra.circle(-270, 45)
    terra.circle(-270//2, 75)
    terra.end_fill()
    
def gajah():
    terra.penup()
    terra.home()
    terra.right(90)
    terra.forward(30)
    terra.right(90)
    terra.forward(7)
    terra.left(90)
    terra.pendown()
    terra.color("#f6e7cf", "black")
    terra.begin_fill()
    terra.left(90)
    terra.forward(25)
    terra.right(90)
    terra.forward(50)
    terra.right(90)
    terra.forward(50)
    terra.right(90)
    terra.forward(50)
    terra.right(90)
    terra.forward(25)
    terra.end_fill()

    terra.penup()
    terra.forward(5)
    terra.right(90)
    terra.forward(5)
    terra.color("#f6e7cf","#f6e7cf")
    terra.pendown()
    terra.begin_fill()
    terra.forward(5)
    terra.left(90)
    terra.forward(5)
    terra.left(90)
    terra.forward(5)
    terra.left(90)
    terra.forward(5)
    terra.end_fill()

    terra.penup()
    terra.forward(10)
    terra.pendown()
    terra.begin_fill()
    terra.forward(5)
    terra.left(90)
    terra.forward(5)
    terra.left(90)
    terra.forward(5)
    terra.left(90)
    terra.forward(5)
    terra.end_fill()

    terra.penup()
    terra.right(180)
    terra.forward(10)
    terra.color("#f6e7cf", "black")
    terra.pendown()
    terra.begin_fill()
    terra.forward(70)
    terra.left(90)
    terra.forward(5)
    terra.left(90)
    terra.forward(70)
    terra.left(90)
    terra.forward(5)
    terra.end_fill()




