def ellips(x):
    terra.penup()
    terra.right(x)
    terra.forward(200)
    terra.right(90)
    terra.forward(100)
    terra.color("black", "yellow")
    terra.pendown()
    terra.begin_fill()
    terra.seth(45)
    terra.circle(-270, 90)
    terra.circle(-270 // 2, 90)
    terra.circle(-270, 90)
    terra.circle(-270 // 2, 90)
    terra.end_fill()
    
def globe(x):
    terra.penup()
    terra.home()
    terra.right(x)
    terra.forward(170)
    terra.right(90)
    terra.forward(5)
    terra.right(90)
    terra.pendown()
    terra.forward(350)
    terra.color("black", "black")
    terra.begin_fill()
    terra.seth(0)
    terra.circle(-175, 180)
    terra.right(90)
    terra.forward(4)
    terra.right(90)
    terra.circle(171, 180)
    terra.end_fill()
    terra.right(170)
    terra.begin_fill()
    terra.circle(-60, 90)
    terra.circle(-700, 20)
    terra.circle(-60, 90)
    terra.right(90)
    terra.forward(4)
    terra.right(90)
    terra.circle(57,90)
    terra.circle(691,20)
    terra.circle(57,90)
    terra.end_fill()

    terra.left(77.5)
    terra.forward(50)
    terra.left(90)
    terra.begin_fill()
    terra.circle(190, 30)
    terra.circle(25, 45)
    terra.left(90)
    terra.forward(3)
    terra.left(90)
    terra.circle(-22, 45)
    terra.circle(-187, 30)
    terra.end_fill()

    terra.left(92.5)
    terra.forward(50)
    terra.left(90)
    terra.begin_fill()
    terra.circle(295, 30)
    terra.circle(25, 45)
    terra.left(90)
    terra.forward(3)
    terra.left(90)
