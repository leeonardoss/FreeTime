import turtle

terra = turtle.Turtle()
def palu():
    terra.penup()
    terra.home()
    terra.right(90)
    terra.forward(10)
    terra.left(90)
    terra.pendown()
    terra.color("black", "black")
    terra.begin_fill()
    terra.forward(10)
    terra.left(90)
    terra.forward(30)
    terra.left(90)
    terra.forward(7)
    terra.right(90)
    terra.forward(60)
    terra.right(90)
    terra.forward(5)
    terra.right(5)
    terra.forward(5)
    terra.right(5)
    terra.forward(10)
    terra.right(5)
    terra.forward(10)
    terra.right(5)
    terra.forward(20)
    terra.left(170)
    terra.forward(56)
    terra.left(20)
    terra.forward(20)
    terra.left(5)
    terra.forward(20)
    terra.left(5)
    terra.forward(20)
    terra.left(90)
    terra.forward(30)
    terra.left(90)
    terra.forward(15)
    terra.left(75)
    terra.forward(10)
    terra.right(75)
    terra.forward(25)
    terra.right(90)
    terra.forward(62)
    terra.right(90)
    terra.forward(7)
    terra.left(90)
    terra.forward(30)
    terra.end_fill()
